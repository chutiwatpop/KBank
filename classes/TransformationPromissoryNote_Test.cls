private class TransformationPromissoryNote_Test {
	@isTest
    static void testPromissoryNoteTransform_Positive() {
        SingleRequestMock fncResponse = new SingleRequestMock(200, 'Complete', lendingPNResponse(), null);
		SingleRequestMock lpmResponse = new SingleRequestMock(200, 'Complete', lpmResponse(), null);
		Map<String, HttpCalloutMock> endpointRespMap = new Map<String,HttpCalloutMock>();
        endpointRespMap.put(SME_CIRestWebservice.FNC_ENDPOINT, fncResponse);
        endpointRespMap.put(SME_CIRestWebservice.LPM_TDR_ENDPOINT, lpmResponse);
        HttpCalloutMock multiCalloutMock = new MultiRequestMock(endpointRespMap);
        Test.setMock(HttpCalloutMock.class, multiCalloutMock);

        Test.startTest();
        String cisId = '12792873';
        IProductUsageConfig productUsageConfig = new ProductUsageConfig();
        IProductUsageTransformation productUsageTransformer = new TransformationPromissoryNote(cisId, 'PN', productUsageConfig);
		ProductUsageController.ProductUsageModel productUsages = productUsageTransformer.transform();
		ProductUsageController.ProductUsageTable productUsageTable = productUsages.productUsageTables.get('PN_0');
        ProductUsageController.ProductUsageTable productUsageTablelevelAccount = productUsages.productUsageTables.get('PN_1417001707002001_3');
        Test.stopTest();

        system.assertEquals(12, productUsages.productUsageTables.size());
		system.assertEquals(0, productUsageTable.columnHeaders.size());
		system.assertEquals(4, productUsageTable.body.size());
        system.assertEquals(15, productUsageTable.body[0].size());
        system.assertEquals(1, productUsageTablelevelAccount.body.size());
        system.assertEquals(16, productUsageTablelevelAccount.body[0].size());
    }

